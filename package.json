    "@koishijs/plugin-teach": "3.2.0",
    "@koishijs/plugin-verifier": "1.0.5",
    "@sinonjs/fake-timers": "^9.1.2",
    "koishi": "^4.7.4",
    "koishi-plugin-baidu": "1.0.3",
    "koishi-plugin-dice": "^1.0.0",
    "koishi-plugin-hitokoto": "2.1.4",
    "koishi-plugin-meme": "2.2.3",
    "lunar-javascript": "^1.2.32"
  },
  "yakumo": {
    "require": [
      "esbuild-register",
      "yml-register"
    ]
  }
}
