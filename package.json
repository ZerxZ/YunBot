{
  "name": "koishi-boilerplate",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "build": "koishi-scripts build",
    "clean": "koishi-scripts clean",
    "new": "koishi-scripts new",
    "setup": "koishi-scripts setup",
    "bump": "yakumo version",
    "dep": "yakumo upgrade",
    "pub": "yakumo publish",
    "dev": "cross-env NODE_ENV=development koishi start -r esbuild-register -r yml-register --watch",
    "start": "koishi start",
    "mirai": "cd mirai && go-cqhttp"
  },
  "devDependencies": {
    "@koishijs/client": "^3.4.1",
    "@koishijs/scripts": "^2.0.1",
    "@types/node": "^17.0.35",
